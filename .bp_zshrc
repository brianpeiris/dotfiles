function start_ssh_agent {
    if [ -z "$SSH_AGENT_PID" ]; then
        eval `ssh-agent -s` > /dev/null
        ssh-add
    fi
}
alias sa=start_ssh_agent

function find_computer {
    for i in {1..25};
    do
        ssh -o ConnectTimeout=1 192.168.1.$i 'uname -a' | grep $1
        if [ $? = 0 ]; then
            break
        fi
    done
    echo $i;
}
alias fc=find_computer

